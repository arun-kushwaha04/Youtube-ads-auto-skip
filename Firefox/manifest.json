{
 "manifest_version": 3,
 "name": "Auto Skip Youtube Ads",
 "version": "2.0",
 "action": {
  "default_icon": "icons/icon.png"
 },
 "description": "Auto skips youtube ads",

 "icons": {
  "48": "icons/icon.png"
 },

 "content_scripts": [
  {
   "matches": ["*://*.youtube.com/*"],
   "js": ["skipper.js"]
  }
 ]
}
