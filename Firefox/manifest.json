{
 "manifest_version": 2,
 "name": "Auto Skip Youtube Ads",
 "version": "3.25",
 "browser_action": {
  "default_icon": "icons/icon.png"
 },
 "description": "Auto skips youtube ads",
 "icons": {
  "48": "icons/icon.png"
 },
 "content_scripts": [
  {
   "matches": ["*://*.youtube.com/*"],
   "js": ["skipper.js"]
  }
 ]
}
